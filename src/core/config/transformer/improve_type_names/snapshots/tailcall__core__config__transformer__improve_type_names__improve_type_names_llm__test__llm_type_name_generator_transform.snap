schema @server @upstream {
  query: Query
}

type Address {
  city: String
  geo: Location
  street: String
  suite: String
  zipcode: String
}

type Location {
  lat: String
  lng: String
}

type Query {
  f1: [User] @http(baseURL: "https://jsonplaceholder.typicode.com", path: "/users")
}

type Quote {
  bs: String
  catchPhrase: String
  name: String
}

type User {
  address: Address
  company: Quote
  email: String
  id: Int
  name: String
  phone: String
  username: String
  website: String
}
